class Solution {
  public:
    int rotateDelete(vector<int> &arr) {
        // Your code here
        int n=arr.size();
        int p=n;
        int q=1;
        for(int i=0;i<n/2;i++)
        {
            int temp=arr[p-1];
            for(int j=p-1;j>0;j--)
            {
                arr[j]=arr[j-1];
            }
            arr[0]=temp;
            arr.erase(arr.end()-q);
            p--;
            q++;
        }
        return arr[0];
    }
};
